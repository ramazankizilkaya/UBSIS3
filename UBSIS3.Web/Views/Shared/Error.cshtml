@{
    Layout = "_Layout";
}

@section Head {
    <title>@ViewBag.Error</title>
    <meta name="description" content="@ViewBag.Description" />
}


<section id="page-title" class="page-title-parallax page-title-dark" style="background-image: url('/images/parallax.jpg'); padding: 120px 0;" data-bottom-top="background-position:0px 300px;" data-top-bottom="background-position:0px -300px;">

    <div class="container clearfix">
        <h1>@ViewBag.Error</h1>
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="/">@ViewBag.Home</a></li>
            <li class="breadcrumb-item active" aria-current="page">@ViewBag.Exception</li>
        </ol>
    </div>
</section>


<section id="content">

    <div class="content-wrap">

        <div class="container clearfix">

            <div class="col_half nobottommargin">
                <div class="error404 center">404</div>
            </div>

            <div class="col_half nobottommargin col_last">

                <div class="heading-block nobottomborder">
                    <h4>@ViewBag.Oops</h4>
                    <span>@ViewBag.ErrorMessage.</span>
                </div>
                <a href="/" type="button" class="btn btn-outline-primary">@ViewBag.Home</a>
            </div>

        </div>

    </div>

</section>